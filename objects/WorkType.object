    <validationRules>
        <fullName>Cannot_Change_Service_App_Job_Name</fullName>
        <active>true</active>
        <description>This Validation Rule prevents users from changing the name of the &quot;Service App Job&quot; Work Type because it is used in various pieces of automation which would break if the name was changed.</description>
        <errorConditionFormula>AND(
ISCHANGED(Name),
CONTAINS( PRIORVALUE(Name) ,&quot;Service App Job&quot;) )</errorConditionFormula>
        <errorMessage>You cannot change the name of this Work Type because it is utilized in automation within the org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_Modification_Sys_Admins_Only</fullName>
        <active>true</active>
        <description>Only system administrators may modify the work type name.</description>
        <errorConditionFormula>AND(NOT(OR( 
$Profile.Name=&quot;System Administrator&quot;, $Permission.Work_Types_Bypass_Validation_Rules =TRUE,$Profile.Name=&quot;Accel Call Center&quot;)), 
ISCHANGED( Name ))</errorConditionFormula>
        <errorMessage>You do not have permission to edit this field. Please contact your system administrator.</errorMessage>
    </validationRules>
