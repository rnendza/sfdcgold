    <fields>
        <fullName>Projected_Budget_Spend__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(Marketing_Request__r.Approvals_Complete__c = TRUE, Marketing_Request__r.RecordTypeId &lt;&gt;&quot;0121I000000W9xV&quot;, Marketing_Request__r.RecordTypeId&lt;&gt;&quot;0121I000000W9xX&quot;, Marketing_Request__r.RecordTypeId&lt;&gt;&quot;0121I000000ecyt&quot;,Marketing_Request__r.RecordTypeId&lt;&gt;&quot;0121I000000BQPQ&quot;,DATEVALUE(  Marketing_Request__r.CreatedDate ) &gt;= DATE(2020,01,01),
DATEVALUE( CreatedDate ) &gt;= DATE(2020,01,01)),
 (Marketing_Request__r.Total_Approved_Amount__c /Marketing_Request__r.MRP_Rollup_Count__c),0)


/*If Co-Op, then use actual Reimbursed By Accounting Amount only.
If Promo, if approvals complete, then use Accel Portion of Cost Share plus actual Reimbursed By Accounting Amount.
If Brand Ambassador or Standard, and approvals complete, then Price * Quantity.
If Standard Custom Size or Customized, and approvals complete, then MR.Project Total Cost to Accel/MRP Rollup Count.*/



/*Original formula before 7/15/2020 meeting with Amber:

CASE( Marketing_Product_Type__c,
&quot;Co-Op&quot;,
 (Marketing_Request__r.Amount_Paid_By_Accel__c/Marketing_Request__r.MRP_Rollup_Count__c),

&quot;Promotions&quot;,
IF( Marketing_Request__r.Approvals_Complete__c = TRUE, 
( Marketing_Request__r.Accel_Portion_of_Cost_Share__c + Marketing_Request__r.Total_Reimbursed_By_Accounting__c)/
Marketing_Request__r.MRP_Rollup_Count__c,0),

&quot;Brand Ambassador Events&quot;,
IF(Marketing_Request__r.Approvals_Complete__c = TRUE,
(Price__c * Quantity__c),0),

&quot;Standard&quot;,
IF(Marketing_Request__r.Approvals_Complete__c = TRUE,
(Price__c * Quantity__c),0),

&quot;Standard Custom Size&quot;,
IF(Marketing_Request__r.Approvals_Complete__c = TRUE,
 (  Marketing_Request__r.Amount_Paid_By_Accel__c   /Marketing_Request__r.MRP_Rollup_Count__c),0),

&quot;Customized&quot;,
IF(Marketing_Request__r.Approvals_Complete__c = TRUE,
 ( Marketing_Request__r.Amount_Paid_By_Accel__c  /Marketing_Request__r.MRP_Rollup_Count__c),0),

0)*/</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Projected Budget Spend MRP</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
