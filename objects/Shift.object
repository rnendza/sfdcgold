    <validationRules>
        <fullName>Confirmed_Shifts_Have_Service_Region</fullName>
        <active>false</active>
        <description>Check to verify that Confirmed shifts have the Service Region populated.</description>
        <errorConditionFormula>ISPICKVAL(Status,&apos;Confirmed&apos;) &amp;&amp; ISPICKVAL(Service_Region__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Confirmed Shifts must have Service Region populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Overtime_Approver_Permission_Set_Needed</fullName>
        <active>true</active>
        <description>Validation rule to check is the user creating the Overtime Shift has the permission to do it</description>
        <errorConditionFormula>AND(ISPICKVAL( TimeSlotType , &quot;Extended&quot;),  NOT(($Permission.Field_Service_Overtime_Approver)))</errorConditionFormula>
        <errorDisplayField>TimeSlotType</errorDisplayField>
        <errorMessage>You are not approved to create an Overtime Shift. Please contact your Salesforce Admin with further questions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Overtime_Reason_Needed</fullName>
        <active>true</active>
        <description>Overtime Reason needs to be filled out if an overtime shift is selected</description>
        <errorConditionFormula>AND( ISPICKVAL(TimeSlotType, &quot;Extended&quot;), ISBLANK(  Overtime_Reason__c ))</errorConditionFormula>
        <errorDisplayField>Overtime_Reason__c</errorDisplayField>
        <errorMessage>Overtime Reason must be filled out in order to create an Overtime Shift</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Secondary_Same_as_Primary_STM</fullName>
        <active>true</active>
        <description>Checks to see if the Primary and Secondary Territory match.</description>
        <errorConditionFormula>Text(Secondary_Service_Territory__c)=Primary_Service_Territory__c</errorConditionFormula>
        <errorDisplayField>Secondary_Service_Territory__c</errorDisplayField>
        <errorMessage>Service Resource is already assigned to this Territory as their Primary Territory. Please leave this field blank, select a different Secondary Territory, or change their Primary Service Territory.</errorMessage>
    </validationRules>
</CustomObject>
