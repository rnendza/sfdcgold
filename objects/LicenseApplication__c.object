    <validationRules>
        <fullName>Approved_Status</fullName>
        <active>false</active>
        <errorConditionFormula>IF(OR(Text(Stage__c) = &apos;Approved&apos; &amp;&amp; isBlank(License_Number__c), Text(Appeal_Stage__c) = &apos;Approved&apos; &amp;&amp; isBlank(License_Number__c)), True, False)</errorConditionFormula>
        <errorDisplayField>License_Number__c</errorDisplayField>
        <errorMessage>Must Enter License Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Status_before_Submitted_Date</fullName>
        <active>false</active>
        <errorConditionFormula>And(Isblank(text(Stage__c)), Not(ISBlank( Submitted_Date__c )))</errorConditionFormula>
        <errorDisplayField>Stage__c</errorDisplayField>
        <errorMessage>Enter Stage before Submitted Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_Stage</fullName>
        <active>false</active>
        <errorConditionFormula>CASE(   Stage__c  ,
&quot;Submitted&quot;,1,
&quot;Pending&quot;,2,
&quot;Approved&quot;,3,
&quot;Rejected&quot;,4,
&quot;Closed&quot;,5,0)
&gt;
CASE (TEXT(PRIORVALUE(Stage__c)) ,
&quot;Submitted&quot;,1,
&quot;Pending&quot;,2,
&quot;Approved&quot;,3,0)</errorConditionFormula>
        <errorMessage>Follow the Application Process - Please submit the Application first</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventStageSkip</fullName>
        <active>false</active>
        <errorConditionFormula>IF( CreatedDate =  LastModifiedDate &amp;&amp; (Text( Stage__c )= &apos;Pending&apos; || Text(Stage__c)==&apos;Approved&apos;), True, False)</errorConditionFormula>
        <errorMessage>Test Error</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Approve_to_Rejected_or_Appeal</fullName>
        <active>false</active>
        <errorConditionFormula>IF( OR(Text(Stage__c)=&quot;Rejected&quot; &amp;&amp; Text(PRIORVALUE(Stage__c))=&quot;Approved&quot;, Text(Stage__c)=&quot;Appeal&quot; &amp;&amp; Text(PRIORVALUE(Stage__c))=&quot;Approved&quot;) , TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Application is Already Approved. Cannot be Rejected or Appealed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STI_Form_Submitted_must_be_completed</fullName>
        <active>false</active>
        <errorConditionFormula>Isblank(STI_Form_Submitted__c) &amp;&amp;  Isblank(EFT_Form_Submitted__c) &amp;&amp; Text(Type__c)=&quot;GLC_COAM_Class&quot; &amp;&amp;  Not(Isblank(Submitted_Date__c))</errorConditionFormula>
        <errorMessage>EFT Form Submitted AND STI Form Submitted must be complete PRIOR to the Application Submitted Date.</errorMessage>
    </validationRules>
