        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Date_Bt_Most_Recent_HPD_and_3_Mths__c</fullName>
        <description>Allows us to figure out 12 week Averages in SOQL using the data available as opposed to calendar time.</description>
        <externalId>false</externalId>
        <formula>/**
 *  @TODO note how to account for days the location is not open?
 *
 *  IF RT is monthly (ie account)
 *    Return True if the date on the hpd record is within 3 months of the most recent hpd record on the account.
 *  IF RT is daily (ie asset)
 *    Return true if the date on the hpd record is within 90 days of the most recent hpd record on the account.
 *
 *  @see HpdSvc.class (Used in nightly aggregation of hpd data)
 */
IF(  LOWER(RecordType.Name) = &apos;monthly hpd&apos;,
        Date__c &gt;= ADDMONTHS(  Account__r.Most_Recent_Hold_Per_Day__r.Date__c,-2),
