    <fields>
        <fullName>Collection_Cycle_Number__c</fullName>
        <description>The position of this route to be collected in the 28 day cycle (4 weeks, 7 day per week)</description>
        <externalId>false</externalId>
        <label>Collection Cycle Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Collection_Date__c</fullName>
        <externalId>false</externalId>
        <label>Collection Date ( Obsolete? )</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Cycle_Number__c</fullName>
        <description>The position of this route to be created in the 28 day cycle (4 weeks, 7 day per week)</description>
        <externalId>false</externalId>
        <label>Fill Cycle Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Geolocation__c</fullName>
        <displayLocationInDecimal>false</displayLocationInDecimal>
        <externalId>false</externalId>
        <label>Current Geolocation</label>
        <required>false</required>
        <scale>5</scale>
        <trackTrending>false</trackTrending>
        <type>Location</type>
    </fields>
    <fields>
        <fullName>Next_Collection_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Mod(TODAY() +1  - Origination_Date__c,28) &lt; Collection_Cycle_Number__c, TODAY() + Collection_Cycle_Number__c - Mod(TODAY()+1 - Origination_Date__c,28),
If(Mod(TODAY() +1  - Origination_Date__c,28) &gt; Collection_Cycle_Number__c, TODAY() + 28 - Mod(TODAY()+1 - Origination_Date__c,28) + Collection_Cycle_Number__c, TODAY()))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Collection Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Next_Fill_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Mod(TODAY() - Origination_Date__c,28) +1 = Cycle_Number__c, TODAY(), IF(Mod(TODAY() - Origination_Date__c,28) +1 &lt; Cycle_Number__c, TODAY() + Cycle_Number__c - (Mod(TODAY() - Origination_Date__c,28) +1), TODAY() + 27 - Mod(TODAY() - Origination_Date__c,28) + Cycle_Number__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Fill Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Normal_Fill_Cycle_Number__c</fullName>
        <description>Used as a temporary field to assist in mass changing fill cycle numbers temporarily for Holiday changes</description>
        <externalId>false</externalId>
        <label>Normal Fill Cycle Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
