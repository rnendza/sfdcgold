    <fields>
        <fullName>Missing_Vehicle_Start_Info__c</fullName>
        <description>Used primarily for ui help to determine if vehicle data has been entered</description>
        <externalId>false</externalId>
        <formula>/* =====================================
 * Checks to see of all vehicle data is entered.
 *
 * pseudo:
 *  If no value for one the below below the box.
 *   1. Vehicle Make (Vehicle__c)
 *   2. Assigned Driver
 *   3. Vehicle License
 *   4. Vehicle Mileage
 * =====================================
 */
OR( ISBLANK( TEXT(Vehicle__c) )
    ,ISBLANK( Assigned_Driver__c )
    ,ISBLANK( Starting_Vehicle_Milage__c )
    ,Starting_Vehicle_Milage__c == 0
    ,ISBLANK( Vehicle_License__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Is any Vehicle information populated?</inlineHelpText>
        <label>Missing Vehicle / Start Info</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
