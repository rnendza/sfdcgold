    <validationRules>
        <fullName>Ensure_Approved_Before_Entering_PO_Date</fullName>
        <active>true</active>
        <description>Ensures the Inventory Request was approved before the user enters the PO  Submitted Date</description>
        <errorConditionFormula>/*===============================================================================
 * Ensures the following:
 *
 * If the Inventory Request contains a Po Submitted Date...
 *
 *  1. the Inventory_Request_Status__c value != &apos;Open&apos; AND != &apos;Pending Approval&apos; 
 *  2. AND the record type is Gaming_Software.
 *  3. AND Bypass_Validation_Rules__c is not = TRUE
 *
 *================================================================================
 * 
 */
 IF( !ISBLANK( PO_Submitted_Date__c ),
AND(  OR(TEXT(Status__c) = &apos;Open&apos;,TEXT(Status__c) = &apos;Pending Approval&apos;) , RecordType.DeveloperName = &apos;Gaming_Software&apos;,Bypass_Validation_Rules__c = FALSE),FALSE)</errorConditionFormula>
        <errorDisplayField>PO_Submitted_Date__c</errorDisplayField>
        <errorMessage>The Inventory Request Must be Approved before entering the PO Submitted date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Approved_Before_Entering_PO_Nbr</fullName>
        <active>true</active>
        <description>Ensures the Inventory Request was approved before the user enters the PO Number</description>
        <errorConditionFormula>/*===============================================================================
 * Ensures the following:
 * 
 *
 * If the Inventory Request contains a Po Number...
 *
 *  1. the Inventory_Request_Status__c value != &apos;Open&apos; AND != &apos;Pending Approval&apos; (it&apos;s an Order of Operations thing with process builder)
 *  2. AND the record type is Gaming_Software.
 *  3. AND Bypass_Validation_Rules__c is not = TRUE
 *
 *================================================================================
 * 
 */
 IF( !ISBLANK( PO_Number__c ),  
AND(  OR(TEXT(Status__c) = &apos;Open&apos;,TEXT(Status__c) = &apos;Pending Approval&apos;) , RecordType.DeveloperName = &apos;Gaming_Software&apos;,Bypass_Validation_Rules__c = FALSE),FALSE)</errorConditionFormula>
        <errorDisplayField>PO_Number__c</errorDisplayField>
        <errorMessage>The Inventory Request Must be Approved before entering the PO Number</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
