/* For Promotions, if approvals are all complete, takes the requested reimbursement amount plus the accel portion of cost share (to accomodate both reimbursements and split costs items within the promotion. */ 
IF(AND 
(RecordType.Name =&apos;Promotions&apos;,Approvals_Complete__c = TRUE),( Requested_Reimbursement_Amount__c + Accel_Portion_of_Cost_Share__c ), 

/* For sweepstakes. &quot;Date sent to Accounting&quot; denotes approval by Accounting. Takes the Total Cost of Items and subtracts the locations cost share where applicable. */
IF(AND 
((NOT(ISBLANK( Date_Sent_to_Accounting__c))), 
RecordType.Name =&apos;Sweepstakes&apos;,( Total_Cost_of_Items__c- Location_Cost_Share_Amount_Rollup__c )&gt;0),( Total_Cost_of_Items__c- Location_Cost_Share_Amount_Rollup__c ), 
