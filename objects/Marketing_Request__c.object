    <validationRules>
        <fullName>Assigned_MC_restricted</fullName>
        <active>true</active>
        <description>To restrict the Assigned Marketing Coordinator to only certain Marketing Users</description>
        <errorConditionFormula>AND(OR($User.Id =&quot;0051I000002FbQQ&quot;),
ISCHANGED(  Assigned_Marketing_Coordinator__c  ))</errorConditionFormula>
        <errorMessage>You do not have sufficient permissions to modify this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Start_Month_and_Year_Required</fullName>
        <active>true</active>
        <description>If value &quot;50/50 Content Split&quot; is chosen on Location Content Split, Billing Start Month and Billing Start Year are required to be populated.</description>
        <errorConditionFormula>ispickval( Location_Content_Split__c , &quot;50/50 Content Split&quot;)
&amp;&amp;
(ispickval( Billing_Start_Month__c,&quot;&quot;)
||
ispickval( Billing_Start_Year__c,&quot;&quot; ))</errorConditionFormula>
        <errorMessage>Please, enter Billing Start Month and Billing Start Year information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_submit_invoice_without_approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Proof_of_Payment__c = TRUE,  Approvals_Complete__c = FALSE)</errorConditionFormula>
        <errorMessage>Approvals of cost must be obtained prior to submitting the invoice.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_Products_Cannot_Be_Chosen</fullName>
        <active>false</active>
        <description>If not a Standard or Promotional request, multiple products cannot be chosen</description>
        <errorConditionFormula>and(RecordTypeId  &lt;&gt;&quot;0121I000000W9xZ&quot;,RecordTypeId  &lt;&gt;&quot;0121I000000W9km&quot;,MRP_Rollup_Count__c &gt;1)</errorConditionFormula>
        <errorMessage>For this record type, you cannot choose more than one Marketing Request Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor_Needed_accel_ent_only</fullName>
        <active>false</active>
        <description>If record type is Accel Only and Vendor Needed field is not filled out</description>
        <errorConditionFormula>and(TEXT(Vendor_Needed__c) &lt;&gt; &apos;Yes&apos;,
TEXT(Vendor_Needed__c) &lt;&gt; &apos;No&apos;,
 RecordType.Name = &quot;Accel Entertainment (Office Use Only)&quot;)</errorConditionFormula>
        <errorMessage>Please choose whether or not a vendor is needed. Field: Vendor Needed</errorMessage>
    </validationRules>
