(RecordType.Name =&apos;Standard&apos;, 0, 
IF(AND 
((NOT(ISBLANK(Project_Completed_Date__c))), 
RecordType.Name =&apos;Customized&apos;,Approvals_Complete__c = TRUE), ( Location_Cost_Share_Amount__c), 
IF(AND 
((NOT(ISBLANK(Project_Completed_Date__c))), 
RecordType.Name =&apos;Standard Custom Size&apos;), 0, 
IF 
(RecordType.Name =&apos;Accel Entertainment (Office Use Only)&apos;,0, 
IF(AND 
(RecordType.Name =&apos;Co-Op&apos;,Approvals_Complete__c = TRUE,Total_Reimbursed_By_Accounting__c&gt;0), (Total_Quote_Amount__c - Requested_Reimbursement_Amount__c ),
IF(AND 
((NOT(ISBLANK( Date_Check_Issued__c))), 
RecordType.Name =&apos;Co-Op&apos;,Total_Reimbursed_By_Accounting__c=0), (Total_Quote_Amount__c-Requested_Reimbursement_Amount__c ),  
IF(AND 
(RecordType.Name =&apos;Promotions&apos;,Approvals_Complete__c = TRUE),(( Total_Quote_Amount__c - Requested_Reimbursement_Amount__c)+( Location_Cost_Share_Amount__c )) , 
IF(AND 
((NOT(ISBLANK( Date_Sent_to_Accounting__c ))), 
RecordType.Name =&apos;Sweepstakes&apos;), Location_Cost_Share_Amount__c, 
0))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
