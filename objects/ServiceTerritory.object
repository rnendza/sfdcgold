    <validationRules>
        <fullName>Require_Restock_WH_if_Live_with_WM</fullName>
        <active>true</active>
        <description>This Validation Rule ensures that, if a Service Territory has the &quot;Live with WM Scheduling Enhancements&quot; checkbox checked, the &quot;Restock Warehouse&quot; field is populated.</description>
        <errorConditionFormula>AND(
Live_with_WM_Scheduling_Enhancements__c = TRUE,
ISBLANK(Restock_Warehouse__c))</errorConditionFormula>
        <errorMessage>Because this Service Territory has the &quot;Live with WM Scheduling Enhancements&quot; checkbox checked, you must populate the &quot;Restock Warehouse&quot; field.</errorMessage>
    </validationRules>
