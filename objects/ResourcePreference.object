NOT($Profile.Name = &quot;System Administrator&quot; ), 
OR(
ISCHANGED( RelatedRecordId), 
ISCHANGED( PreferenceType ), 
ISCHANGED(  ServiceResourceId ))),
true, false)</errorConditionFormula>
