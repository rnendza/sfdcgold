    <validationRules>
        <fullName>Fill_out_FTE_Count</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Type__c ,&quot;Part-Time&quot;), isblank(FTE_Count__c))</errorConditionFormula>
        <errorDisplayField>FTE_Count__c</errorDisplayField>
        <errorMessage>Please fill out the FTE Count field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hiring_Reason_New</fullName>
        <active>true</active>
        <errorConditionFormula>and(ispickval( Hiring_Reason__c,&quot;New&quot;), isblank( Explanation_of_Hiring_Need__c ),RecordTypeId = &quot;0121I000000WQPE&quot;)</errorConditionFormula>
        <errorDisplayField>Explanation_of_Hiring_Need__c</errorDisplayField>
        <errorMessage>If your hiring reason is New, please fill out the Explanation of Hiring Need</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hiring_Reason_Replacement</fullName>
        <active>true</active>
        <description>for hiring reason = replacement, need name of person replacing
HR requested to have the field highlighted, and that is why this val rule and the &quot;replacement_Reason_required&quot; were created separately</description>
        <errorConditionFormula>and(
ispickval(Hiring_Reason__c,&quot;Replacement - Separation&quot;),
RecordType.DeveloperName=&apos;Position_Requisition&apos;,
isblank(Replacing_name_of_person__c)
)</errorConditionFormula>
        <errorDisplayField>Replacing_name_of_person__c</errorDisplayField>
        <errorMessage>Your Hiring Reason is &quot;Replacement&quot;.
Please fill out the name of the &quot;Replacing (name of person)&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Job_Bad_Values_for_Sales_Incentive_Plan</fullName>
        <active>true</active>
        <description>prevent users from choosing N/A along with other values</description>
        <errorConditionFormula>and(
 INCLUDES( Sales_Incentive_Plan__c , &quot;N/A&quot;) , 

or(

 INCLUDES( Sales_Incentive_Plan__c , &quot;BDM Commissions Agreement IL&quot;),
 INCLUDES( Sales_Incentive_Plan__c , &quot;BDM Commissions Agreement GA&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;RM Commissions Agreements&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;GA AM Incentive&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;SDR Incentives&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;NE BDM Incentive&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;NE AM Incentive&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;NC BDM Incentive&quot;), 
 INCLUDES( Sales_Incentive_Plan__c , &quot;Other Commission Agreement&quot;)


))</errorConditionFormula>
        <errorDisplayField>Sales_Incentive_Plan__c</errorDisplayField>
        <errorMessage>When choosing N/A for a Sales Incentive Plan, you cannot choose additional values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Job_No_Longer_Needed_requires_a_reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 Job_No_Longer_Needed__c = TRUE ,
 ISBLANK( Explanation_of_Inactive_Requisition__c ) )</errorConditionFormula>
        <errorDisplayField>Explanation_of_Inactive_Requisition__c</errorDisplayField>
        <errorMessage>A reason for inactivating a job is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Replacement_Reason_required</fullName>
        <active>true</active>
        <description>for hiring reason = replacement, need Replacement Reason
HR requested to have the field highlighted, and that is why this val rule and the &quot;Hiring_Reason_Replacement&quot; were created separately</description>
        <errorConditionFormula>and(
ispickval(Hiring_Reason__c,&quot;Replacement - Separation&quot;),
RecordType.DeveloperName=&apos;Position_Requisition&apos;,
ispickval( Replacement_Reason__c ,&quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Replacement_Reason__c</errorDisplayField>
        <errorMessage>Your Hiring Reason is &quot;Replacement&quot;.
Please select a value for the &quot;Replacement Reason&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Agent_field_must_be_populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Human_Resources_Approval__c = TRUE,ISBLANK( text(Need_to_register_as_employee_sales_agent__c )))</errorConditionFormula>
        <errorMessage>Please fill out the following field: &quot;Need to register as employee sales agent.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transfer_Reason_is_required</fullName>
        <active>true</active>
        <description>Transfer Reason” is required for “Transfer” Hiring Reason</description>
        <errorConditionFormula>and(
ispickval( Hiring_Reason__c,&apos;Replacement - Transfer Department&apos;),
ispickval( Transfer_Reason__c ,&apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Transfer_Reason__c</errorDisplayField>
        <errorMessage>Please, select a Transfer Reason.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
