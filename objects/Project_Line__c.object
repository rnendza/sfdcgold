    <validationRules>
        <fullName>AddSwapReconfigs_Need_to_have_a_Type</fullName>
        <active>true</active>
        <description>If the project record type is add/swap/reconfigure, the project line item type needs to be populated.</description>
        <errorConditionFormula>AND(Project__r.RecordTypeId = &quot;0127A0000009alx&quot;,ISblank(TEXT(( Project_Line_Type__c ))))</errorConditionFormula>
        <errorDisplayField>Project_Line_Type__c</errorDisplayField>
        <errorMessage>Please choose a Project Line Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Lines_creation_or_editing</fullName>
        <active>true</active>
        <errorConditionFormula>AND(not(ispickval(Project__r.Project_Type__c,&quot;Gaming Area Assets&quot;)), NOT( $Profile.Name = &quot;Accel Super User&quot;),NOT( $Profile.Name = &quot;Accel Sales Super User&quot;),NOT( $Profile.Name = &quot;System Administrator&quot;))</errorConditionFormula>
        <errorMessage>You are not allowed to edit this project line. Please contact Salesforce Support if you think you should have access.</errorMessage>
    </validationRules>
