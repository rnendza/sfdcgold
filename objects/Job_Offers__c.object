    <validationRules>
        <fullName>Employment_Agreement_is_Required</fullName>
        <active>true</active>
        <description>If job offer status = offer extended, you must fill out Employment Agreement</description>
        <errorConditionFormula>AND(
ISPICKVAL( Job_Offer_Status__c , &quot;Offer Extended&quot;) ,
ISPICKVAL( Employment_Agreement__c ,&quot;&quot;) )</errorConditionFormula>
        <errorDisplayField>Employment_Agreement__c</errorDisplayField>
        <errorMessage>When Job Offer Status is Offer Extended, an Employment Agreement value is required.
Please, enter an Employment Agreement value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Proposed_Start_Date_must_be_filled_out</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Job_Offer_Status__c , &quot;Accepted&quot;),ISBLANK( Proposed_Start_Date__c ))</errorConditionFormula>
        <errorMessage>In order to mark this Job Offer as Accepted, please fill out a Proposed Start Date. This date can be changed later if needed.</errorMessage>
    </validationRules>
