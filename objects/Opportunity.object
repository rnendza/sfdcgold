    <validationRules>
        <fullName>Date_Agreement_Signed_Stage_Dependent</fullName>
        <active>true</active>
        <description>Enforce that there is a value in the field &quot;Date Agreement Signed,&quot; before the Opp can advance to Eligibility or Internal Review stage. RTs= AE Video Gaming and AE Non Video Gaming.  (Unless it&apos;s Closed Lost then bypass)</description>
        <errorConditionFormula>/*
 *  IF the Record Type is in [AE Video Gaming, AE Non Video Gaming]
 *      IF the Stage is after Eligibility or Internal Review.ie. NOT in [Prepare Sales Strategy,Customer Meeting/Proposal,Negotiation/Handling Objections] 
 *          IF it is not Closed Lost   &lt;-----   New addition 3/15/2021
 *              IF The Opp.Date_Agreement_Signed__c = NULL
 *
 *                 THROW A VALIDATION ERROR!
 */
AND( 
 ISBLANK(Date_Agreement_Signed__c) 
,OR( RecordType.Name =&apos;AE Video Gaming&apos;
    ,RecordType.Name =&apos;AE Non Video Gaming&apos;)
,NOT(  
          CASE(TEXT(StageName),
            &quot;Prepare Sales Strategy&quot;,1,
            &quot;Customer Meeting/Proposal&quot;,1,  
            &quot;Negotiation/Handling Objections&quot;,1,
            &quot;Closed Lost&quot;,1,    /* &lt;-- New Addition 3/15/2021  bypass Closed Lost */
            0) == 1
   )
)</errorConditionFormula>
        <errorDisplayField>Date_Agreement_Signed__c</errorDisplayField>
        <errorMessage>Please enter a value for Date Agreement Signed</errorMessage>
