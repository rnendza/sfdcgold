    <validationRules>
        <fullName>Require_Parent_Warehouse_for_Remote_Dpt</fullName>
        <active>true</active>
        <description>This Validation Rule ensures that the Parent Warehouse field is populated for all Warehouse records with a type of &quot;Remote Parts Depot.&quot;</description>
        <errorConditionFormula>AND(
 TEXT(LocationType) = &quot;Remote Parts Depot&quot;,
ISBLANK( ParentLocationId ))</errorConditionFormula>
        <errorMessage>Because this record has a Type of &quot;Remote Parts Depot,&quot; you must populate the Parent Warehouse field, which represents the warehouse which gets restocked if a tech uses a part from this Remote Parts Depot.</errorMessage>
    </validationRules>
