    <validationRules>
        <fullName>Cannot_Edit_After_Approval_Action</fullName>
        <active>true</active>
        <description>Editing of the record is not allowed after a a value has been chosen for Approval Action.</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
NOT(ISBLANK( TEXT(Approval_Action__c) )),
NOT(ISBLANK(PRIORVALUE(Approval_Action__c)))
)</errorConditionFormula>
        <errorMessage>This request can no longer be edited because a decision was made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Does_Key_Gaming_Decision_Maker_Approve</fullName>
        <active>true</active>
        <description>If the contact is not a key gaming decision maker, user needs to answer &quot;Does Key Gaming Decision Maker Approve?&quot;</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Is_Contact_Key_Gaming_Decision_Maker__c , &quot;No&quot;) ,

 ISBLANK(TEXT( Does_Key_Gaming_Decision_Maker_Approve__c) ) 

)</errorConditionFormula>
        <errorDisplayField>Does_Key_Gaming_Decision_Maker_Approve__c</errorDisplayField>
        <errorMessage>Please, enter a value for &quot;Does Key Gaming Decision Maker Approve?&quot; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_Rejection_is_required</fullName>
        <active>true</active>
        <description>Reason for Rejection is required when the Approval Action = Rejected</description>
        <errorConditionFormula>AND(ISPICKVAL( Approval_Action__c , &quot;Rejected&quot;),ISBLANK( Reason_for_Rejection__c ) )</errorConditionFormula>
        <errorDisplayField>Reason_for_Rejection__c</errorDisplayField>
        <errorMessage>Please enter a reason for the rejection of this PCR.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
