    public static String generateId() {
        return generateHexDigest('MD5', Blob.valueOf(DateTime.now().getTime().format()));
    }

    /**
     * @param algorithmName Name to generate the crypto digest for: [MD5,SHA1,SHA3-256,SHA3-384,SHA3-512,SHA-256SHA-512]
     * @param b             The blob to create a hex digest for.
     *
     * @return
     */
    public static String generateHexDigest(String algorithmName, Blob b){
        return EncodingUtil.convertToHex(Crypto.generateDigest(algorithmName,b));
    }

