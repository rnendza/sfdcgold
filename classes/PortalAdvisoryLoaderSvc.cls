            user.userPhysicalDesc.heightDisplay = user.userPhysicalDesc.heightFt+'-'+user.userPhysicalDesc.heightIn;
        }
        user.userPhysicalDesc.distinguishingMarks = (String)mPhysicalDesc.get('DistinguishingMarks');
    }

