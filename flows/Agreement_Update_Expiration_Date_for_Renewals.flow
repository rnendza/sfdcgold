<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <description>this flow updates the Agreement Expiration Date to reflect the Agreement Auto Renew Length. The update is done on the Agreement Auto-Renew Date
2/27/2023: Added COAM GA Class B and Gaming Agreements in entry criteria</description>
    <environments>Default</environments>
    <formulas>
        <description>get the number value from Agreement Auto Renew Length Month</description>
        <name>fx_Agreement_Auto_Renew_Length_Month</name>
        <dataType>Number</dataType>
        <expression>VALUE(TEXT({!$Record.Agreement_Auto_Renew_Length__c}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>fx_Date_After_Renewal</name>
        <dataType>Date</dataType>
        <expression>TODAY()-1</expression>
    </formulas>
    <formulas>
        <name>fx_new_agreement_expiration_date</name>
        <dataType>Date</dataType>
        <expression>addmonths({!var_current_agreement_expiration_date},({!fx_Agreement_Auto_Renew_Length_Month}*12))</expression>
    </formulas>
    <interviewLabel>Agreement: Update Expiration Date for Renewals {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Agreement: Update Expiration Date for Renewals</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>update the Agreement Expiration Date to be extended the number of years of Agreement Auto Renew Length</description>
        <name>Update_Agreement_Expiration_Date</name>
        <label>Update Agreement Expiration Date</label>
        <locationX>176</locationX>
        <locationY>276</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Agreement_Auto_Renew_Date__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>fx_Date_After_Renewal</elementReference>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Active</stringValue>
            </value>
        </filters>
        <filters>
            <field>Agreement_Termination_Date__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <inputAssignments>
            <field>Agreement_End_Date__c</field>
            <value>
                <elementReference>fx_new_agreement_expiration_date</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Update_Agreement_Expiration_Date</targetReference>
        </connector>
        <filterLogic>1 and (2 OR 3 or 4 or 5 or 6)</filterLogic>
        <filters>
            <field>Agreement_Auto_Renew_Length__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>0121I000000W9xGQAS</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>0121I000000W9xEQAS</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>0121I000000W9xBQAS</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>0128W000001QH3fQAG</stringValue>
            </value>
        </filters>
        <filters>
            <field>RecordTypeId</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>0121I000000BZSSQA4</stringValue>
            </value>
        </filters>
        <object>Contract</object>
        <schedule>
            <frequency>Daily</frequency>
            <startDate>2022-08-19</startDate>
            <startTime>05:00:00.000Z</startTime>
        </schedule>
        <triggerType>Scheduled</triggerType>
    </start>
    <status>Active</status>
    <variables>
        <description>store the current agreement expiration date</description>
        <name>var_current_agreement_expiration_date</name>
        <dataType>Date</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <value>
            <elementReference>$Record.Agreement_End_Date__c</elementReference>
        </value>
    </variables>
</Flow>
