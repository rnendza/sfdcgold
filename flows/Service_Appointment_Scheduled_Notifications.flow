    <decisions>
        <name>Does_Assigned_Resource_Exist</name>
        <label>Does Assigned Resource Exist?</label>
        <locationX>999</locationX>
        <locationY>213</locationY>
        <defaultConnector>
            <targetReference>Log_No_Assigned_Resource</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Assigned_Resource_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AssignedResource.ServiceResource.RelatedRecordId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Eligible_to_Send_Notification</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Eligible_to_Send_Notification</name>
        <label>Is Eligible to Send Notification</label>
        <locationX>1379</locationX>
        <locationY>201</locationY>
        <defaultConnector>
            <targetReference>Log_Service_Resource_Not_Eligible</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No - Not Eligible</defaultConnectorLabel>
        <rules>
            <name>Yes_Send_Notification_Eligible</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ServiceResource.Live_with_WM_Scheduling_Enhancements__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Resource_as_Recipient</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Send_Attempted_Successfully</name>
        <label>Send Attempted Successfully?</label>
        <locationX>1692</locationX>
        <locationY>551</locationY>
        <defaultConnector>
            <targetReference>Log_Error_on_Attempted_Send</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Send Attempt Failed</defaultConnectorLabel>
        <rules>
            <name>Yes_Send_Attempted</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varOutputIsSendSuccess</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Custom_Notifications_Sent_Log</targetReference>
            </connector>
            <label>Yes Send Attempted</label>
        </rules>
    </decisions>
