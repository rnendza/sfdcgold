        </defaultConnector>
        <defaultConnectorLabel>Partner Operator Can Handle</defaultConnectorLabel>
        <rules>
            <name>Partner_Operator_Cannot_Handle</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>AssigningCase</leftValueReference>
                <operator>EqualTo</operator>
