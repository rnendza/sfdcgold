    <decisions>
        <name>All_Games_Down_Checked</name>
        <label>All Games Down Checked?</label>
        <locationX>446</locationX>
        <locationY>1598</locationY>
        <defaultConnector>
            <targetReference>Asset_Choice_is_Selected</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>All_Games_Down_is_Chosen</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Account_Assets</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>All_Games_Down_Selection</elementReference>
                </rightValue>
