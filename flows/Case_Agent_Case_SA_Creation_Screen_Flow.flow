                <targetReference>All_Games_Down_Assignment</targetReference>
            </connector>
            <label>All Games Down is Chosen</label>
        </rules>
        <rules>
            <name>Chosen_Asset_is_Redemption</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Selected_Asset_Fields.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Redemption</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>getAccountRecord.Number_of_RT_Assets__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>1.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>RT_no_backup_Service_Category_Default</targetReference>
            </connector>
            <label>Chosen Asset is Redemption</label>
        </rules>
