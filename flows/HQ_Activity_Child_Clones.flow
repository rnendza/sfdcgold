            <value>
                <elementReference>varEventId</elementReference>
            </value>
        </filters>
        <object>Event</object>
        <outputAssignments>
            <assignToReference>varDescription</assignToReference>
            <field>Description</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>varDidTheyShowUp</assignToReference>
            <field>Did_They_Showup__c</field>
        </outputAssignments>
        <outputAssignments>
