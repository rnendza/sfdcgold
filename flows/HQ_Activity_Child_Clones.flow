    <description>Upon choosing &quot;Corporate Call&quot; on a parent account, this flow will clone the event to all child accounts.</description>
    <formulas>
        <name>MinutesAgo59</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} - (1/25)</expression>
    </formulas>
    <interviewLabel>HQ Activity - Child Clones {!$Flow.CurrentDateTime}</interviewLabel>
    <label>HQ Activity - Child Clones</label>
    <loops>
        <name>Loop_Through_Child_Accounts</name>
        <label>Loop Through Child Accounts</label>
        <locationX>526</locationX>
        <locationY>336</locationY>
        <assignNextValueToReference>ChildCollectionRecordVar</assignNextValueToReference>
        <collectionReference>varChildAcctCollectionId</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Assign_Event_Elements</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Fast_Create_Event_Clones</targetReference>
        </noMoreValuesConnector>
    </loops>
