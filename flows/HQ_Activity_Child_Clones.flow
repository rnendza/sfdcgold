        </outputAssignments>
        <outputAssignments>
            <assignToReference>varWhatId</assignToReference>
            <field>WhatId</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>varWhoId</assignToReference>
            <field>WhoId</field>
        </outputAssignments>
    </recordLookups>
